{
    "pipeline_setting": {
        "train": true,
        "test": false,
        "checkpoint_dir": "./checkpoint/model/k15_type2_fold4/",
        "load_pretrained": false,
        "resume": false,
        "state_dict": "",
        "name": "OneToManyTrainer",
        "loop": "train_loop",
        "dataset": "OneToManyDataset",
        "model_arch": "OneToManyModel",
        "model": "microsoft/deberta-v3-large",
        "pooling": "SubSequenceGEMPooling"
    },
    "common_settings": {
        "wandb": true,
        "optuna": false,
        "competition": "CommonLit",
        "seed": 42,
        "n_gpu": 1,
        "gpu_id": 0,
        "num_workers": 4
    },
    "data_settings": {
        "n_folds": 7,
        "max_len": 2048,
        "epochs": 30,
        "batch_size": 8,
        "val_batch_size": 8
    },
    "gradient_settings": {
        "amp_scaler": true,
        "gradient_checkpoint": true,
        "clipping_grad": true,
        "n_gradient_accumulation_steps": 1,
        "max_grad_norm": 100
    },
    "loss_options": {
        "loss_fn": "RMSELoss",
        "val_loss_fn": "RMSELoss",
        "reduction": "none"
    },
    "metrics_options": {
        "metrics": "MCRMSELoss"
    },
    "optimizer_options": {
        "optimizer": "AdamW",
        "llrd": true,
        "layerwise_lr": 5e-5,
        "layerwise_lr_decay": 0.9,
        "layerwise_weight_decay": 1e-2,
        "layerwise_adam_epsilon": 1e-8,
        "layerwise_use_bertadam": false,
        "betas": [0.9, 0.999]
    },
    "scheduler_options": {
        "scheduler": "cosine_annealing",
        "batch_scheduler": true,
        "num_cycles": 3,
        "warmup_ratio": 0.1
    },
    "swa_options": {
        "swa": false,
        "swa_lr": 5e-6,
        "anneal_epochs": 1,
        "anneal_strategy": "cos"
    },
    "model_utils": {
        "attention_probs_dropout_prob": 0.1,
        "hidden_dropout_prob": 0.1,
        "init_weight": "kaiming_uniform",
        "stop_mode": "min",
        "freeze": false,
        "num_freeze": 4,
        "reinit": true,
        "num_reinit": 1,
        "awp": false,
        "nth_awp_start_epoch": 2,
        "awp_eps": 1e-2,
        "awp_lr": 1e-4
    }
}